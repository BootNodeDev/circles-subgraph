  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.15.3",
    "@graphprotocol/graph-ts": "^0.15.1",
    "truffle": "^5.0.40"
  },
  "dependencies": {
    "circles-contracts": "^1.0.5",
    "web3": "^1.2.1"
  }
